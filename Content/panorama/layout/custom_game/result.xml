<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/result.css" />
		<include src="file://{resources}/styles/dotastyles.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/result.js" />
	</scripts>
	<Panel class="result">
		<Panel class="result-top">
			<Label class="result-header" text="#result_header" />
			<Label class="result-close-button" text="#close_button" onactivate="CloseResult()" />
		</Panel>
		<Panel class="result-bottom">
			<Label class="result-body" html="true" />
			<DOTAHeroImage class="result-target" heroimagestyle="portrait" />
		</Panel>
	</Panel>
</root>